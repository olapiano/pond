var h√§lsa = 0;
var hastighet = 100;
var maxSteg = 100;
var steg = 0;
var riktning = 0;

while(true)
{
  simma();
  
}

function simma() 
{
  while(steg < maxSteg)
  {
    swim(riktning, hastighet);
    steg++;
    if(getX() > 80) riktning = Math.random() * 30 + 165;
    if(getX() < 20) cannon(90, 50);
    if(getY() > 80) cannon(90, 50);
    if(getX() > 20) cannon(90, 50);
  }
  steg = 0;
  riktning = Math.random() * 360;
  stop();
}
